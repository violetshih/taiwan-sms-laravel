{
    "name": "violetshih/taiwan-sms-laravel",
    "description": "Laravel wrapper package for the taiwan sms services",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Violetshih\\TaiwanSmsLaravel\\": "src/"
        }
    },
    "version": "1.1.4",
    "autoload-dev": {
        "psr-4": {
            "Violetshih\\TaiwanSmsLaravel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Violetshih",
            "email": "sd12sd@gmail.com"
        }
    ],
    "require": {
        "infobip/infobip-api-php-client": "^3.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Violetshih\\TaiwanSmsLaravel\\TaiwanSmsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.24",
        "pestphp/pest": "^1.21"
    }
}
